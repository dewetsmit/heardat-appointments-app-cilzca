{
  "expo": {
    "name": "Heardat Appointments",
    "slug": "heardatappointments",
    "version": "2.2.3",
    "orientation": "portrait",
    "icon": "./assets/images/Heardat_icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/Heardat_icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "heardat.app.ionic",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/Heardat_icon.png",
        "backgroundColor": "#000000"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.heardat.app"
    },
    "web": {
      "favicon": "./assets/images/Heardat_icon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-font",
      "expo-router",
      "expo-web-browser",
      "./app.plugin.js",
      [
        "react-native-edge-to-edge",
        {
          "android": {
            "enforceNavigationBarContrast": false
          }
        }
      ]
    ],
    "scheme": "heardatappointments",
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "backendUrl": "https://mt2nx2p2qg45rgu2mbbrhzpk8hd3se69.app.specular.dev"
    }
  }
}