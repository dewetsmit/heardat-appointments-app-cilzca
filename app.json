{
  "expo": {
    "name": "Heardat Appointments",
    "slug": "Heardat-Appointments",
    "version": "2.2.3",
    "orientation": "portrait",
    "icon": "./assets/images/AppIcon-512@2x.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/AppIcon-512@2x.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "heardat.app.ionic",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/AppIcon-512@2x.png",
        "backgroundColor": "#000000"
      },
      "edgeToEdgeEnabled": true,
      "package": "heardat.app.ionic"
    },
    "web": {
      "favicon": "./assets/images/Heardat_icon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-font",
      "expo-router",
      "expo-web-browser"
    ],
    "scheme": "heardatappointments",
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "backendUrl": "https://ah6xbghrnu8mtkscsqwnfchq7r74g6tx.app.specular.dev"
    }
  }
}